		<span class="evento_item_titulo"><%=h evento.data.strftime("%d/%m/%Y") %>
		<br/>
		<span class="evento_item_desc"><%= link_to evento.titulo, evento %></span>
		</span>
		<br />
		<br />
<%= link_to_remote 'teste ' + evento.id.to_s, :update => 'fotos_content', :url =>{ :controller=> 'eventos', :action => 'teste', :id => evento.id} %>
	</div>
<% end %>
